<% @surveys.each do |survey| %>
  <h5 id="title_id"><%= survey.title %></h5>
    <a href="/surveys/results/<%= URI.escape(survey.title, Regexp.new("[^#{URI::PATTERN::UNRESERVED}]")) %>"><font size="3">Results</font></a>
    <% if logged_in? && current_user.can_take_survey?(survey) %>
        <a href="/surveys/<%= URI.escape(survey.title, Regexp.new("[^#{URI::PATTERN::UNRESERVED}]")) %>"><font size="3">Take Survey</font></a>
    <% end %>
<%end%>
